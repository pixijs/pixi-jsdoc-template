<?js
    var self = this;
    function compare_access(a,b) {
        if ( a.inherited !== b.inherited ) {
            if ( a.inherited ) {
                return 1;
            } else if ( b.inherited ) {
                return -1;
            }
        }
        
        if ( a.overrides !== b.overrides ) {
            if ( a.overrides ) {
                return 1;
            } else if ( b.overrides ) {
                return -1;
            }
        }
        
        if ( !a.access || a.access === "" ) {
            a.access = "public";
        }
        if ( !b.access || b.access === "" ) {
            b.access = "public";
        }
        
        if ( a.access !== b.access ) {
            if ( a.access === "public" ) {
                return -1;
            } else if ( b.access === "public" ) {
                return 1;
            }
        }
        
        if (a.longname < b.longname) {
            return -1;
        } else if (a.longname > b.longname) {
            return 1;
        } else {
            return 0;
        }
    }
    
    docs.forEach(function(doc, i) {
?>

